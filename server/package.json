{
  "name": "mapme",
  "version": "1.0.0",
  "description": "simple node with typescript and graphql",
  "main": "index.js",
  "author": "Mohammed alreai",
  "license": "MIT",
  "scripts": {
    "dev": "  cd src && nodemon --exec ts-node index.ts  -e ts,graphql",
    "pretype": " gql-merge --output-file ./src/schema.graphql ./src/api/**.*.graphql",
    "type": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.2",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.4",
    "graphql-yoga": "^1.18.3",
    "helmet": "^4.1.1",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28"
  }
}
